<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dariooliveirajr.dmesp">

    <application
        android:name=".MyApplication"
        android:icon="@drawable/icon"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:hardwareAccelerated="false"
        android:largeHeap="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.dariooliveirajr.dmesp.ContatoActivity"></activity>

        <activity android:name="com.dariooliveirajr.dmesp.CronogramaActivity"></activity>

        <activity android:name="com.dariooliveirajr.dmesp.EscolaActivity"></activity>

        <activity android:name="com.dariooliveirajr.dmesp.LicitacaoActivity"></activity>

        <activity android:name="com.dariooliveirajr.dmesp.PaisActivity"></activity>

        <activity android:name="com.dariooliveirajr.dmesp.PlannerActivity"></activity>

        <activity android:name="com.dariooliveirajr.dmesp.SobreActivity"></activity>
    </application>

</manifest>