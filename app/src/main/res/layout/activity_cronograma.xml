<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.dariooliveirajr.dmesp.CronogramaActivity"
    android:background="@drawable/fundo"
    android:padding="0dp">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:id="@+id/imageView6"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/retangulo2" />

    <ImageView
        android:layout_width="350dp"
        android:layout_height="400dp"
        android:id="@+id/imageView16"
        android:background="@drawable/cronograma3"
        android:layout_below="@+id/imageView15"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:id="@+id/imageView7"
        android:background="@drawable/topo2"
        android:layout_alignTop="@+id/imageView6"
        android:layout_alignBottom="@+id/imageView6"
        android:layout_toEndOf="@+id/btnVoltaIdCron"
        android:layout_toRightOf="@+id/btnVoltaIdCron" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:id="@+id/imageView15"
        android:background="@drawable/cronograma1"
        android:layout_below="@+id/imageView6"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnVoltaIdCron"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/volta"
        android:layout_above="@+id/imageView15" />


</RelativeLayout>
