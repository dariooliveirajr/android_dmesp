<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dariooliveirajr.dmesp">

    <application
        android:name=".MyApplication"
        android:icon="@drawable/icon"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ContatoActivity"></activity>

        <activity android:name=".CronogramaActivity"></activity>

        <activity android:name=".EscolaActivity"></activity>

        <activity android:name=".LicitacaoActivity"></activity>

        <activity android:name=".PaisActivity"></activity>

        <activity android:name=".PlannerActivity"></activity>

        <activity android:name=".SobreActivity"></activity>
    </application>

</manifest>